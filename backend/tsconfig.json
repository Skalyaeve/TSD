{
  "compilerOptions": {
    "module": "es2022",
    "declaration": true, //?
    "removeComments": true, //?
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true, //?
    "target": "es2022",
    "esModuleInterop": true,
    "outDir": "./dist",
    "baseUrl": "./", //?
    "incremental": true, //? c koi sa?
    "skipLibCheck": true, //?
    "strictNullChecks": false,  //?
    "noImplicitAny": false,  //?
    "strictBindCallApply": false,  //? c koi sa?
    "noFallthroughCasesInSwitch": false, //?
		"moduleResolution": "node16",
  },
  "exclude": [
    "node_modules",
    "test", 
    "dist", 
    "**/*spec.ts",
    "headless",
    "controller"
  ]
}